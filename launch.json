// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app", // Cambia 'main' por el nombre de tu archivo principal
        "--reload"
      ],
      "jinja": true
    }
  ]
}